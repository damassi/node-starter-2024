{
  "name": "node-starter-2024",
  "version": "0.0.1",
  "description": "Node.js starter app",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "vite build",
    "dev": "tsx watch --ignore ./vite.config.js.timestamp-* --ignore 'src/client/*' src/server.ts --dev",
    "docs": "open http://localhost:3000/docs",
    "generate-schema": "tsx src/server.ts --generate-schema && npx openapi-typescript ./src/schema/api-schema.json --output ./src/schema/api-schema.ts",
    "lint": "eslint --fix",
    "postinstall": "npm run generate-schema",
    "prepare": "husky",
    "prettier": "prettier --write",
    "start": "npm run dev",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "type": "module",
  "author": "Christopher Pappas <damassi.pappas@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/one-line-logger": "^1.4.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.0.1",
    "@fastify/vite": "^6.0.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastify": "^4.28.1",
    "openapi-fetch": "^0.11.1",
    "openapi-schema": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.17.0",
    "vite-express": "^0.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "fastify-cli": "^6.3.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.9",
    "openapi-typescript": "^7.3.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.1",
    "whatwg-fetch": "^3.6.20"
  }
}
